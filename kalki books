#include<stdio.h>
int main()
{
    int a,b,c;
    scanf("%d%d",&a,&b);
    printf("enter the first edition copies :\n",a);
    printf("enter a second edition copiies:\n",b);
    c=a+b;
    printf("total copies=%d",c);
    return 0;
    
}



---------------------------------------------------------------------------
output:
enter the first edition copies:10
enter the second edition copies:20
total copies=30
